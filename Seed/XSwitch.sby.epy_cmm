[options]
mode bmc
depth 100

[engines]
#abc bmc3
smtbmc boolector

[script]
read_verilog -sv -formal <%=cmm_prefix%>req_XMatrix.sv <%=cmm_prefix%>req_XArbiter_RR.sv <%=cmm_prefix%>req_XArbiter.sv <%=cmm_prefix%>req_XRouter.sv 
read_verilog -sv -formal <%=cmm_prefix%>rsp_XMatrix.sv <%=cmm_prefix%>rsp_XArbiter_RR.sv <%=cmm_prefix%>rsp_XArbiter.sv <%=cmm_prefix%>rsp_XRouter.sv 
read_verilog -sv -formal <%=cmm_prefix%>XSwitch.sv
prep -top <%=cmm_prefix%>XSwitch

[files]
<%=cmm_prefix%>req_XMatrix.sv
<%=cmm_prefix%>req_XArbiter_RR.sv
<%=cmm_prefix%>req_XArbiter.sv
<%=cmm_prefix%>req_XRouter.sv
<%=cmm_prefix%>rsp_XMatrix.sv
<%=cmm_prefix%>rsp_XArbiter_RR.sv
<%=cmm_prefix%>rsp_XArbiter.sv
<%=cmm_prefix%>rsp_XRouter.sv
<%=cmm_prefix%>XSwitch.sv
